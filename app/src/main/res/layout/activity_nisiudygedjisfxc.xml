<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/bg_mr_fr"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:fontFamily="@font/mys_f_bold_it"
        android:text="Choose miracle item and try your luck"
        android:textColor="@color/white"
        android:textSize="19sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/huczxiczoxksk"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="60dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:srcCompat="@drawable/mr_f_1" />

    <ImageView
        android:id="@+id/llxzocokzxjisjia"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/huczxiczoxksk"
        app:layout_constraintStart_toEndOf="@+id/huczxiczoxksk"
        app:layout_constraintTop_toTopOf="@+id/huczxiczoxksk"
        app:srcCompat="@drawable/mr_f_2" />

    <ImageView
        android:id="@+id/kmcmzxmkcmkzxusauh"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/llxzocokzxjisjia"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/llxzocokzxjisjia"
        app:srcCompat="@drawable/mr_f_3" />

    <ImageView
        android:id="@+id/rokkodsfkooksdfok"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="30dp"
        app:layout_constraintEnd_toEndOf="@+id/huczxiczoxksk"
        app:layout_constraintStart_toStartOf="@+id/huczxiczoxksk"
        app:layout_constraintTop_toBottomOf="@+id/huczxiczoxksk"
        app:srcCompat="@drawable/mr_f_5" />

    <ImageView
        android:id="@+id/ncxnvbxcusdhijsd"
        android:layout_width="120dp"
        android:layout_height="120dp"
        app:layout_constraintBottom_toBottomOf="@+id/rokkodsfkooksdfok"
        app:layout_constraintEnd_toEndOf="@+id/llxzocokzxjisjia"
        app:layout_constraintStart_toStartOf="@+id/llxzocokzxjisjia"
        app:layout_constraintTop_toTopOf="@+id/rokkodsfkooksdfok"
        app:srcCompat="@drawable/mr_f_6" />

    <ImageView
        android:id="@+id/odkkofdkosfko"
        android:layout_width="120dp"
        android:layout_height="120dp"
        app:layout_constraintBottom_toBottomOf="@+id/ncxnvbxcusdhijsd"
        app:layout_constraintEnd_toEndOf="@+id/kmcmzxmkcmkzxusauh"
        app:layout_constraintStart_toStartOf="@+id/kmcmzxmkcmkzxusauh"
        app:layout_constraintTop_toTopOf="@+id/ncxnvbxcusdhijsd"
        app:srcCompat="@drawable/mr_f_4" />

    <com.google.android.material.card.MaterialCardView
        android:layout_width="350dp"
        app:cardCornerRadius="10dp"
        app:cardBackgroundColor="@color/prm_clr_mira"
        android:layout_height="100dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ncxnvbxcusdhijsd" >

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="You have only one chance to choose"
                android:textSize="20sp"
                android:fontFamily="@font/mys_f_bold"
                android:textColor="@color/white"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </com.google.android.material.card.MaterialCardView>

</androidx.constraintlayout.widget.ConstraintLayout>