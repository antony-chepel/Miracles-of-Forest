        android:textColor="@color/white"
        android:text="Your forest scores is - 2000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tokkosdfkokodf" />

    <Button
        android:id="@+id/sdokskkowko"
        android:layout_width="250dp"
        android:layout_height="60dp"
        android:text="Try Again"
        android:backgroundTint="@color/prm_clr_mira"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/kfodkocxvkook"
        app:layout_constraintVertical_bias="0.3" />
</androidx.constraintlayout.widget.ConstraintLayout>